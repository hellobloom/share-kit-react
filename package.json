{
  "name": "@bloomprotocol/share-kit-react",
  "version": "4.0.0",
  "description": "React wrapper for @bloomprotocol/share-kit",
  "main": "dist/index.js",
  "module": "dist/share-kit.esm.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/hellobloom/share-kit-react",
  "author": "Bloom Team <team@bloom.co>",
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run clean && tsdx watch --tsconfig ./tsconfig.build.json",
    "build": "npm run clean && tsdx build --tsconfig ./tsconfig.build.json",
    "test": "tsdx test --env=jsdom",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run eslint",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "eslint": "./bin/eslint",
    "eslint-fix": "./bin/eslint --fix"
  },
  "dependencies": {
    "@bloomprotocol/share-kit": "^7.0.0"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@storybook/react": "^5.0.11",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "acorn": "^6.1.1",
    "babel-loader": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "jest-canvas-mock": "^2.1.0",
    "jest-resolve": "^24.8.0",
    "prettier": "^1.18.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-testing-library": "^5.4.4",
    "rimraf": "^2.6.2",
    "ts-loader": "^4.5.0",
    "tsdx": "^0.7.2",
    "tslib": "^1.10.0",
    "typescript": "^3.5.2",
    "webpack": "^4.17.1"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
